{
    "name": "sourcebroker/restify",
    "type": "typo3-cms-extension",
    "description": "REST API",
    "authors": [
        {
            "name": "SourceBroker Team",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^9.5",
        "jms/serializer": "^2.3",
        "symfony/routing": "^4.1",
        "symfony/http-foundation": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "SourceBroker\\Restify\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SourceBroker\\Restify\\Tests\\": "Tests"
        }
    },
    "replace": {
        "restify": "self.version",
        "typo3-ter/restify": "self.version"
    }
}
